<!doctype html>
<html metal:use-macro="api.masterTemplate.macros['master']">
<head></head>
<body>


  <metal:navbar fill-slot="navbar">
      <div metal:use-macro="api.masterTemplate.macros['navbar']"></div>
  </metal:navbar>

  <metal:sidebar fill-slot="sidebar">
      <div metal:use-macro="api.masterTemplate.macros['sidebar']"></div>
  </metal:sidebar>

  <metal:main fill-slot="main">

        <ng-hub-info username="${api.authenticated_user.username}"
                     server="${api.application_url}"
                     token="${api.authenticated_user.token}"
                     domain="${api.domain.name}">
        </ng-hub-info>

        <ng-max-info username="${api.authenticated_user.username}"
                     maxserver="${api.domain.max_server}"
                     oauthtoken="${api.authenticated_user.token}">
        </ng-max-info>

        <div ui-view></div>


  </metal:main>


<metal:main fill-slot="view_scripts">

    <script src="${api.application_url}/angular-elastic-input/angular-elastic-input.min.js"></script>

    <script src="${api.application_url}/js/hubClient.js"></script>
    <script src="${api.application_url}/js/MAXClient.js"></script>
    <script src="${api.application_url}/js/sidebar.js"></script>
    <script src="${api.application_url}/maxui/maxui.js"></script>
    <script src="${api.application_url}/js/DomainManagement/app.js"></script>
    <script src="${api.application_url}/js/DomainManagement/api.js"></script>
    <script src="${api.application_url}/js/DomainManagement/users.js"></script>
    <script src="${api.application_url}/js/DomainManagement/contexts.js"></script>
    <script src="${api.application_url}/js/DomainManagement/exceptions.js"></script>

</metal:main>


</body>
</html>

