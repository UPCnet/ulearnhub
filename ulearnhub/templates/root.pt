<!doctype html>
<html metal:use-macro="api.masterTemplate.macros['master']">
<head></head>
<body>

  <metal:main fill-slot="main">
      <div ng-app="uLearnHUB" ng-cloak>

        <ng-hub-info username="${api.authenticated_user}"
                     server="${api.application_url}"
                     token="${api.authenticated_user_token}">
        </ng-hub-info>  

        <div ui-view></div>


      </div>      
  </metal:main>








<metal:main fill-slot="view_scripts">
    <script src="${api.application_url}/js/hubClient.js"></script>
    <script src="${api.application_url}/js/MAXClient.js"></script>
    <script src="${api.application_url}/js/app.js"></script>
    <script src="${api.application_url}/js/domains.js"></script>
    <script src="${api.application_url}/js/users.js"></script>
    <script src="${api.application_url}/js/contexts.js"></script>
</metal:main>


</body>
</html>

